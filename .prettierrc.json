{
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "embeddedLanguageFormatting": "auto",
  "arrowParens": "avoid",
  "bracketSameLine": false,
  "jsxSingleQuote": true,
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "importOrder": ["^(react$)", "<THIRD_PARTY_MODULES>", "^_", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
